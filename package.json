{
  "name": "diagx-omen",
  "version": "0.0.0",
  "private": true,
  "type": "commonjs",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "demo": "node -r ts-node/register src/cli/demo.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "pt": "ts-node src/cli/pt.ts",
